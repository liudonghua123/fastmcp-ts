{
  "name": "fastmcp-ts",
  "version": "0.1.2",
  "description": "typescript version of fastmcp for node",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/",
    "src/",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "src/*": ["src/*"],
      "dist/*": ["dist/*"]
    }
  },
  "scripts": {
    "build": "npm run build:src && npm run build:tests",
    "build:src": "tsc -p tsconfig.src.json",
    "build:tests": "tsc -p tsconfig.tests.json",
    "dev": "tsc -p tsconfig.src.json --watch",
    "test": "node dist/test/my-mcp-server.js",
    "start": "node dist/test/my-mcp-server.js",
    "prepare": "npm run build"
  },
  "author": "liudonghua123",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liudonghua123/fastmcp-ts.git"
  },
  "bugs": {
    "url": "https://github.com/liudonghua123/fastmcp-ts/issues"
  },
  "homepage": "https://github.com/liudonghua123/fastmcp-ts#readme",
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^20.19.15",
    "@typescript-eslint/typescript-estree": "^8.44.0",
    "express": "^5.1.0",
    "pino": "^9.9.5",
    "pino-pretty": "^13.1.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@microsoft/tsdoc": "^0.15.1",
    "@modelcontextprotocol/sdk": "^1.18.0",
    "reflect-metadata": "^0.2.2",
    "zod": "^3.25.76"
  },
  "imports": {
    "#fastmcp": {
      "development": "./src/index.ts",
      "default": "./dist/index.js"
    }
  }
}
