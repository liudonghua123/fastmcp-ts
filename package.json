{
  "name": "fastmcp-ts",
  "version": "0.1.0",
  "description": "typescript version of fastmcp for node",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:src && npm run build:tests",
    "build:src": "tsc -p tsconfig.src.json",
    "build:tests": "tsc -p tsconfig.tests.json",
    "dev": "tsc -p tsconfig.src.json --watch",
    "test": "node dist/test/my-mcp-server.js",
    "start": "node dist/test/my-mcp-server.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^20.19.15",
    "@typescript-eslint/typescript-estree": "^8.44.0",
    "pino": "^9.9.5",
    "pino-pretty": "^13.1.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@microsoft/tsdoc": "^0.15.1",
    "@modelcontextprotocol/sdk": "^1.18.0",
    "express": "^5.1.0",
    "reflect-metadata": "^0.2.2",
    "zod": "^3.25.76"
  },
  "imports": {
    "#fastmcp": {
      "development": "./src/index.ts",
      "default": "./dist/index.js"
    }
  }
}
